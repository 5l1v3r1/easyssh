msgid "Hosting"
msgstr "Хостынг"

#: weblate_web/html/base.html:84
msgid "Develop"
msgstr "Распрацоўваць"

#: weblate_web/html/base.html:85 weblate_web/html/donate.html:54
msgid "Donate"
msgstr "Ахвяраваць"

#: weblate_web/html/base.html:86
msgid "Support"
msgstr "Падтрымка"

#: weblate_web/html/base.html:87
msgid "Tour"
msgstr "Экскурс"

#: weblate_web/html/base.html:88
msgid "Thanks"
msgstr "Падзякі"

